{
  "ConnectionStrings": {
    "StarbaseApiDatabase": "Server=(localdb)\\mssqllocaldb;Database=starbase;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=(localdb)\\mssqllocaldb;Database=starbase;Trusted_Connection=True;",
          "tableName": "Logs",
          "autoCreateSqlTable": true,
          "schemaName": "dbo"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
